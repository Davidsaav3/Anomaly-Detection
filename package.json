{
  "name": "Anomaly-Detection",
  "version": "1.0.0",
  "description": "Processing sensor data using isolation forest algorithm.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Davidsaav3/Anomaly-Detection.git"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "chart.js": "^4.4.4",
    "csv-parser": "^3.0.0",
    "fs": "^0.0.1-security",
    "isolation-forest-visualization": "^1.0.7",
    "mqtt": "^5.10.1"
  },
  "scripts": {
    "sensors 1 - acquisition": "node   ./processing/acquisition.js   ./dataset/acquisition.json   ./sensorsConfig.json",
    "sensors 2 - flatten": "node   ./processing/flatten.js   ./dataset/acquisition.json   ./dataset/flatten.json   ./sensorsConfig.json",
    
    "sensors 3 - expandedColumn": "node   ./processing/expandedColumn.js   ./dataset/flatten.json   ./dataset/expandedColumn.json   ./sensorsConfig.json",
    "sensors 4 - reduceColumn": "node   ./processing/reduceColumn.js   ./dataset/flatten.json   ./dataset/reduceColumn.json   ./sensorsConfig.json",
    
    "sensors 5 - toCsv": "node   ./processing/toCsv.js   ./dataset/expandedColumn.json   ./dataset/toCsv.csv   ./sensorsConfig.json",
        "sensors 5.1 - joinColumn": "node   ./processing/joinColumn.js   ./dataset/toCsv.csv   ./dataset/joinColumn.csv   ./sensorsConfig.json",
    "sensors 6 - encoding": "node   ./processing/encoding.js   ./dataset/toCsv.csv   ./dataset/auxiliaryDictionary.csv    ./dataset/encoding.csv   ./sensorsConfig.json",
        "sensors 6.1 - deleteColumn": "node   ./processing/deleteColumn.js   ./dataset/encoding.csv   ./dataset/deleteColumn.csv   ./sensorsConfig.json",
  
    "sensors 7 - createTemplate": "node   ./processing/createTemplate.js   ./dataset/encoding.csv   ./dataset/auxiliaryNulls.csv   ./dataset/auxiliaryNormalize.csv   ./sensorsConfig.json",
    "sensors 8 - nulls": "node   ./processing/nulls.js   ./dataset/encoding.csv   ./dataset/auxiliaryNulls.csv   ./dataset/nulls.csv    ./sensorsConfig.json",
    "sensors 9 - normalize": "node   ./processing/normalize.js   ./dataset/nulls.csv   ./dataset/auxiliaryNormalize.csv    ./dataset/normalize.csv   ./dataset/auxiliaryMaxMin.csv   ./sensorsConfig.json",
    
    "sensors 10 - addColumn": "node   ./processing/addColumn.js   ./dataset/normalize.csv   ./dataset/addColumn.csv   ./dataset/auxiliaryWeight.csv   ./sensorsConfig.json",
        "sensors 10.1 - createGroups": "node   ./processing/createGroups.js   ./dataset/normalize.csv   ./sensorsConfig.json",
    
    "sensors 11 - index": "node   ./models/isolationForest/index.js   ./dataset/addColumnR.csv   ./dataset/auxiliaryWeight.csv   25   ./models/isolationForest/results/   isolation.csv   scores.csv   metrics.csv    image    ./sensorsConfig.json",
  


    "water 1 - acquisition": "node   ./processing/acquisition.js   ./dataset/acquisition.json   ./waterConfig.json",
    "water 2 - flatten": "node   ./processing/flatten.js   ./dataset/acquisition.json   ./dataset/flatten.json   ./waterConfig.json",
    
    "water 3 - expandedColumn": "node   ./processing/expandedColumn.js   ./dataset/flatten.json   ./dataset/expandedColumn.json   ./waterConfig.json",
    "water 4 - reduceColumn": "node   ./processing/reduceColumn.js   ./dataset/flatten.json   ./dataset/reduceColumn.json   ./waterConfig.json",
    
    "water 5 - toCsv": "node   ./processing/toCsv.js   ./dataset/expandedColumn.json   ./dataset/toCsv.csv   ./waterConfig.json",
        "5.1 - joinColumn": "node   ./processing/joinColumn.js   ./dataset/toCsv.csv   ./dataset/joinColumn.csv   ./waterConfig.json",
    "water 6 - encoding": "node   ./processing/encoding.js    ./dataset/toCsv.csv   ./dataset/auxiliaryDictionary.csv   ./dataset/encoding.csv    ./waterConfig.json",
        "water 6.1 - deleteColumn": "node   ./processing/deleteColumn.js   ./dataset/encoding.csv   ./dataset/deleteColumn.csv   ./waterConfig.json",
  
    "water 7 - createTemplate": "node   ./processing/createTemplate.js   ./dataset/encoding.csv   ./dataset/auxiliaryNulls.csv   ./dataset/auxiliaryNormalize.csv   ./waterConfig.json",
    "water 8 - nulls": "node   ./processing/nulls.js   ./dataset/encoding.csv   ./dataset/auxiliaryNulls.csv   ./dataset/nulls.csv    ./waterConfig.json",
    "water 9 - normalize": "node   ./processing/normalize.js   ./dataset/nulls.csv   ./dataset/auxiliaryNormalize.csv   ./dataset/normalize.csv   ./dataset/auxiliaryMaxMin.csv   ./waterConfig.json",
    
    "water 10 - addColumn": "node   ./processing/addColumn.js   ./dataset/normalize.csv   ./dataset/addColumn.csv   ./dataset/auxiliaryWeight.csv   ./waterConfig.json",
        "water 10.1 - createGroups": "node   ./processing/createGroups.js   ./dataset/normalize.csv   ./waterConfig.json",
    
    "water 11 - index": "node   ./models/isolationForest/index.js   ./dataset/addColumnR.csv   ./dataset/auxiliaryWeight.csv   25   ./models/isolationForest/results/   isolation.csv   scores.csv   metrics.csv    image    ./waterConfig.json"
  
  },
  "keywords": [
    "isolation-forest",
    "sensors",
    "anomaly-detection"
  ],
  "author": {
    "name": "David Saavedra Pastor",
    "email": "Davidsaav3@gmail.com",
